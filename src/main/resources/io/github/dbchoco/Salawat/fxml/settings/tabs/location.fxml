<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<?import io.github.palexdev.materialfx.controls.MFXFilterComboBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="io.github.dbchoco.Salawat.controllers.settings.tabs.LocationController">
    <children>
        <GridPane prefHeight="284.0" prefWidth="293.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>

                <FlowPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Label text="Coordinates" />
                    </children>
                </FlowPane>
                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
                    <children>
                        <MFXTextField fx:id="latField" floatingText="Latitude"/>
                        <MFXTextField fx:id="lonField" floatingText="Longitude"/>
                    </children>
                </FlowPane>

                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                    <children>
                        <Label text="Time zone" />
                    </children>
                </FlowPane>
                <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" GridPane.columnIndex="1">
                    <children>
                        <MFXFilterComboBox floatingText="Timezone"/>
                    </children>
                </FlowPane>

            </children>
        </GridPane>
    </children>
</AnchorPane>
