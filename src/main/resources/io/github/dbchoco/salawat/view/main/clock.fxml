<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="509.0"
            xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="io.github.dbchoco.salawat.controllers.ClockController">
    <Label id="clock" fx:id="clock" styleClass="title" text="00:00:00" />
    <Label id="date" fx:id="date" layoutY="32.0" styleClass="title" text="00/00/0000" />
    <Label id="timeLeft" fx:id="timeLeft" layoutY="58.0" prefHeight="18.0" prefWidth="439.0" text="Time before X: XX:XX:XX" />
</AnchorPane>
