<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="io.github.dbchoco.salawat.controllers.PrayerGridController"
            prefHeight="400.0" prefWidth="600.0">
    <GridPane layoutY="93.0" prefHeight="200.0" prefWidth="200.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <Label fx:id="fajrText" text="Fajr" />
            <Label fx:id="fajrTime" text="00:00" GridPane.columnIndex="1" />
            <Label fx:id="sunriseText" text="Sunrise" GridPane.rowIndex="1" />
            <Label fx:id="sunriseTime" text="00:00" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="dhuhrText" text="Dhuhr" GridPane.rowIndex="2" />
            <Label fx:id="dhuhrTime" text="00:00" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="asrText" text="Asr" GridPane.rowIndex="3" />
            <Label fx:id="asrTime" text="00:00" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="maghribText" text="Maghrib" GridPane.rowIndex="4" />
            <Label fx:id="maghribTime" text="00:00" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label fx:id="ishaText" text="Isha" GridPane.rowIndex="5" />
            <Label fx:id="ishaTime" text="00:00" GridPane.columnIndex="1" GridPane.rowIndex="5" />
        </children>
    </GridPane>
</AnchorPane>
